//TB07052019 REDMINE 3665 EUW - Licence - AP 4% load Provisional 2% discount full licence UK licences NB only.
rule "licence rule"
salience 4600
activation-group "licence rule"
no-loop true
when
	$result : Result(eval(schemes.contains(schemeCode)))
	Policy (rtpType == RTPType.N, $iDate : inceptionDate)
	BaseRisk( affinity != "RIDE" && affinity != "SPEC" && affinity != "RTAR" && affinity !="CLAS")
	Bike ($riskCover : cover)
	$prop : Proposer()
then
	LocalDate $incepDate = new LocalDate($iDate);
	LocalDate $ruleEndDate = new LocalDate("2019-07-01");
	
	if ($incepDate.isBefore($ruleEndDate))
		{
			int $currentUwPremium = $result.getAlgPremium() - $result.getInsurerDiscount().getValue() + $result.getInsurerLoad().getValue();
			int $discountAmount= 0, $loadAmount = 0, $minimumPremium = 6900, $adjuster = 0;
			
			if ($prop.getBikeLicence().getType().equals("F"))
				{	$adjuster = -2;	}
			else if ($prop.getBikeLicence().getType().equals("P"))
				{	$adjuster = 4;	}
			
			if ( $adjuster != 0 )
				{
					int $revisedUwPremium = $currentUwPremium + (($currentUwPremium * $adjuster) / 100);
						
					//Set minimum premium by cover
					if ($riskCover.equals("C"))
						{	$minimumPremium = 6900;	}
					else if ($riskCover.equals("F"))
						{	$minimumPremium = 6400;	}
					else if ($riskCover.equals("T"))
						{	$minimumPremium = 6400;	}
						
					//Check minimums not breached
					if ($revisedUwPremium < $minimumPremium)
						{	$revisedUwPremium = $minimumPremium;	}
									
					if ($revisedUwPremium < $result.getAlgPremium())
						{
							//calc and allow discount and set load to zero
							$discountAmount = $result.getAlgPremium() - $revisedUwPremium;
							InsurerLoad insLoad = new InsurerLoad();
							insLoad.setValue(0);			
							$result.setInsurerLoad(insLoad);
							InsurerDiscount insDis = new InsurerDiscount();
							insDis.setValue($discountAmount);			
							$result.setInsurerDiscount(insDis);
							insDis.setRef("LIC");
							$result.setRulesetChanged(true);
							logger.info("After Licence rule, revised discount allowed of  " + $discountAmount);
						}
					else if ($revisedUwPremium > $result.getAlgPremium())
						{
							//calc and apply load and set discount to zero
							$loadAmount = $revisedUwPremium - $result.getAlgPremium();
							InsurerDiscount insDis = new InsurerDiscount();
							insDis.setValue(0);			
							$result.setInsurerDiscount(insDis);
							InsurerLoad insLoad = new InsurerLoad();
							insLoad.setValue($loadAmount);			
							$result.setInsurerLoad(insLoad);
							insLoad.setRef("LIC");
							$result.setRulesetChanged(true);
							logger.info("After Licence rule, revised load applied of  " + $loadAmount);
						}
					else if ($revisedUwPremium == $result.getAlgPremium())
						{
							//set load and discount to zero
							InsurerDiscount insDis = new InsurerDiscount();
							insDis.setValue(0);			
							$result.setInsurerDiscount(insDis);
							InsurerLoad insLoad = new InsurerLoad();
							insLoad.setValue(0);			
							$result.setInsurerLoad(insLoad);
							$result.setRulesetChanged(true);
							logger.info("After Licence rule, no discount or load allowed" );
						}	
				}
		}	
end

//HN11042017 REDMINE 1788 Update Underwriter Minimum Premium.
//TB03112017 REDMINE 2327 Europa Underwriting - Minimum Premiums - MCD/Ridersure.
rule "apply maximum concession cap"
salience 4500
activation-group "apply maximum concession cap"
no-loop true
when
	$result : Result(eval(schemes.contains(schemeCode)))
	Bike ($cov : cover)
then
	int $currentAlg = $result.getAlgPremium() - $result.getInsurerDiscount().getValue() + $result.getInsurerLoad().getValue();
	int $cappedAlg = $result.getAlgPremium() - (($result.getAlgPremium() * 20) / 100);
	int $discountAmount = 0;
	int $loadAmount = 0;
	int $minPremium = 6900;
	int $revisedDiscount = 0;
	
	//Set minimum premium by cover
	if ($cov.equals("C"))
		{	$minPremium = 6900;	}
	else if ($cov.equals("F"))
		{	$minPremium = 6400;	}
	else if ($cov.equals("T"))
		{	$minPremium = 6400;	}

	if ($result.getInsurerDiscount().getValue() > 0 && ($currentAlg < $cappedAlg))
		{
			$currentAlg = $cappedAlg;

			//check if revised alg post rtp concessions and capped alg check breaches minimum premium
			if ($currentAlg < $minPremium)
				{ $currentAlg = $minPremium;	}

			if ( $currentAlg  < $result.getAlgPremium())
				{
					$discountAmount = $result.getAlgPremium() - $currentAlg  ;
					InsurerLoad insLoad = new InsurerLoad();
					insLoad.setValue(0);
					$result.setInsurerLoad(insLoad);
					InsurerDiscount insDis = new InsurerDiscount();
					insDis.setValue($discountAmount);
					$result.setInsurerDiscount(insDis);
					$result.setRulesetChanged(true);
					logger.info("After concessions cap, revised discount allowed of " + $discountAmount);
