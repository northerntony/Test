rule "insurer loadings until nov2019"
salience 4100
activation-group "insurer loadings until nov2019"
no-loop true
when
	$result : Result(eval(schemes.contains(schemeCode)))
	Policy (rtpType == RTPType.N)
	BaseRisk( affinity != "RIDE" && affinity != "SPEC" && affinity != "RTAR" && affinity !="CLAS")
	Bike ($cov : cover, $bikeMileage : annualMileage)
	Policy($iDate : inceptionDate)
	Proposer ( $proposerAge : age)
then
	LocalDate $incepDate = new LocalDate($iDate);
	LocalDate $loadEndDate = new LocalDate("2019-11-01");

	if ($incepDate.isBefore($loadEndDate))
		{
			int $currentUwPremium = $result.getAlgPremium() - $result.getInsurerDiscount().getValue() + $result.getInsurerLoad().getValue();
			int $loadPercent = 0, $minPremium = 6900;
			int $revisedUwPremium = $currentUwPremium;
			
			if ($proposerAge >= 25 && $proposerAge <= 29)
				{	$loadPercent = $loadPercent + 5;	}	
				
			if ($result.getAreaGroup().equals("C") || $result.getAreaGroup().equals("D") || $result.getAreaGroup().equals("E"))
				{	$loadPercent = $loadPercent + 2;	}
			
			if ($bikeMileage >= 5000)
				{	$loadPercent = $loadPercent + 5;	}
			
			//Set minimum premium by cover
			if ($cov.equals("C"))
				{	$minPremium = 6900;	}
			else if ($cov.equals("F"))
				{	$minPremium = 6400;	}
			else if ($cov.equals("T"))
				{	$minPremium = 6400;	}
			
			if ($loadPercent > 0)
				{	
					//Calculate revised Uw premium using load percent 
					$revisedUwPremium = $currentUwPremium + (($currentUwPremium * $loadPercent) / 100);
						
					//check revised Uw premium does not breach minimum premiums
					if ($revisedUwPremium < $minPremium)
						{	$revisedUwPremium = $minPremium;	}
						
					//work out if there is a load or a discount to apply			
					if ($revisedUwPremium < $result.getAlgPremium())
